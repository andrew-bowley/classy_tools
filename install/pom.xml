<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>au.com.cybersearch2</groupId>
  <artifactId>install-jars</artifactId>
  <version>1.0.1</version>
  <name>Install_Jars</name>
    <build>
      <plugins>
          <plugin>
           <groupId>org.apache.maven.plugins</groupId>
           <artifactId>maven-install-plugin</artifactId>
           <version>2.5.1</version>
           <executions>
              <execution>
                 <id>install1</id>
                 <goals>
                    <goal>install-file</goal>
                 </goals>
                 <phase>initialize</phase>
                 <configuration>
                   <groupId>javax.persistence</groupId>
                   <artifactId>persistence-api</artifactId>
                   <version>2.0</version>
                   <packaging>jar</packaging>
                   <file>${basedir}/persistence-api-2.0.jar</file>
                   <generatePom>true</generatePom>
                 </configuration>
              </execution>
              <execution>
                 <id>install2</id>
                 <goals>
                    <goal>install-file</goal>
                 </goals>
                 <phase>initialize</phase>
                 <configuration>
                   <groupId>com.googlecode.openbeans</groupId>
                   <artifactId>openbeans</artifactId>
                   <version>1.0</version>
                   <packaging>jar</packaging>
                   <file>${basedir}/openbeans-1.0.jar</file>
                   <generatePom>true</generatePom>
                 </configuration>
              </execution>
              <execution>
                 <id>install3</id>
                 <goals>
                    <goal>install-file</goal>
                 </goals>
                 <phase>initialize</phase>
                 <configuration>
                   <packaging>jar</packaging>
                   <file>${basedir}/ormlite-core/ormlite-core.jar</file>
                   <generatePom>false</generatePom>
                   <pomFile>${basedir}/ormlite-core/pom.xml</pomFile>
                 </configuration>
              </execution>
              <execution>
                 <id>install4</id>
                 <goals>
                    <goal>install-file</goal>
                 </goals>
                 <phase>initialize</phase>
                 <configuration>
                   <packaging>jar</packaging>
                   <file>${basedir}/ormlite-jdbc/ormlite-jdbc.jar</file>
                   <generatePom>false</generatePom>
                   <pomFile>${basedir}/ormlite-jdbc/pom.xml</pomFile>
                 </configuration>
              </execution>
              <execution>
                 <id>install5</id>
                 <goals>
                    <goal>install-file</goal>
                 </goals>
                 <phase>initialize</phase>
                 <configuration>
                   <packaging>jar</packaging>
                   <file>${basedir}/ormlite-android/ormlite-android.jar</file>
                   <generatePom>false</generatePom>
                   <pomFile>${basedir}/ormlite-android/pom.xml</pomFile>
                 </configuration>
              </execution> 
              <execution>
                 <id>install6</id>
                 <goals>
                    <goal>install-file</goal>
                 </goals>
                 <phase>initialize</phase>
                 <configuration>
                   <packaging>jar</packaging>
                   <file>${basedir}/support/support-v4-19.0.1.jar</file>
                   <generatePom>false</generatePom>
                   <pomFile>${basedir}/support/support-v4-19.0.1.pom</pomFile>
                 </configuration>
              </execution>
           </executions>
          </plugin>      
        </plugins>
      <pluginManagement>
      	<plugins>
      		<!--This plugin's configuration is used to store Eclipse m2e settings only. It has no influence on the Maven build itself.-->
      		<plugin>
      			<groupId>org.eclipse.m2e</groupId>
      			<artifactId>lifecycle-mapping</artifactId>
      			<version>1.0.0</version>
      			<configuration>
      				<lifecycleMappingMetadata>
      					<pluginExecutions>
      						<pluginExecution>
      							<pluginExecutionFilter>
      								<groupId>
      									org.apache.maven.plugins
      								</groupId>
      								<artifactId>
      									maven-install-plugin
      								</artifactId>
      								<versionRange>
      									[2.5.1,)
      								</versionRange>
      								<goals>
      									<goal>install-file</goal>
      								</goals>
      							</pluginExecutionFilter>
      							<action>
      								<ignore></ignore>
      							</action>
      						</pluginExecution>
      					</pluginExecutions>
      				</lifecycleMappingMetadata>
      			</configuration>
      		</plugin>
      	</plugins>
      </pluginManagement>
    </build>
</project>